{
  "name": "asembli-platform",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "predev": "npm run prisma:generate",
    "prebuild": "npm run prisma:generate",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "db:seed": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" prisma/seed.clean.ts",
    "test:suite": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" test-suite/run-tests.ts",
    "test:messaging": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" test-suite/messaging-isolation.spec.ts",
    "debug:repro-convo": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" scripts/repro-direct-convo.ts",
    "test:donations": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" test-suite/donations-isolation.spec.ts",
    "test:all": "node test-suite/run-tests-with-server.js",
    "test:api": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" -e \"const {TestLogger} = require('./test-suite/test-logger'); const {APITestSuite} = require('./test-suite/api-tests'); const logger = new TestLogger(); const suite = new APITestSuite(logger); suite.runAllTests().then(() => { logger.printSummary(); logger.saveResults(); }).catch(console.error);\"",
    "test:pages": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" -e \"const {TestLogger} = require('./test-suite/test-logger'); const {PageTestSuite} = require('./test-suite/page-tests'); const logger = new TestLogger(); const suite = new PageTestSuite(logger); suite.runAllTests().then(() => { logger.printSummary(); logger.saveResults(); }).catch(console.error);\"",
    "test:features": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" -e \"const {TestLogger} = require('./test-suite/test-logger'); const {FeatureTestSuite} = require('./test-suite/feature-tests'); const logger = new TestLogger(); const suite = new FeatureTestSuite(logger); suite.runAllTests().then(() => { logger.printSummary(); logger.saveResults(); }).catch(console.error);\"",
    "test:ui": "playwright test",
    "test:ui:headed": "playwright test --headed",
    "test:ui:debug": "playwright test --debug",
    "test:setup-users": "ts-node --compiler-options \"{\\\"module\\\":\\\"CommonJS\\\"}\" test-suite/setup-test-users.ts",
    "check-client-imports": "node scripts/check-client-imports.js"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/react": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@jridgewell/sourcemap-codec": "^1.5.5",
    "@prisma/client": "^6.19.0",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/postcss": "^4.1.17",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.553.0",
    "next": "^16.0.3",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.10",
    "prisma": "^6.19.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "^16.0.6",
    "@playwright/test": "^1.56.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.14.0",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2"
  }
}
